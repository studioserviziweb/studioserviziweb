{% spaceless %}

{#
  :: List Main Offices ::

  PARAMS:
  ---- item:															{<string>}
  -------- title:													{<string>}


  USAGE:
  - include
#}
{# Variables set #}
{% set title = contents.title %}
{% set items = contents.item %}

{# Layout #}
{% endspaceless %}<div class="flex-mainoffices container">
<div class="row">
	{% for item in items %}
		<div class="flex-mainoffices-content col-xs-12 col-sm-12 col-md-6">
			<div class="office">
				<h4>{{item.title}}</h4>
				<div class="content">
					{{item.description|raw}}
				</div>
			</div>
		</div>
	{% endfor %}
</div>
</div>
